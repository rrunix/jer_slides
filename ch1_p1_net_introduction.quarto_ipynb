    "    %% Styling\n",
    "    classDef tier1 fill:#ff6b6b,stroke:#d63031,stroke-width:3px,color:#fff\n",
    "    classDef tier2 fill:#4ecdc4,stroke:#00b894,stroke-width:2px,color:#fff\n",
